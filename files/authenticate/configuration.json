{
  "allowed": {
    "session": true,
    "signin": true,
    "signup": true
  },
  "security": {
    "hash": {
      "algorithm": "sha256",
      "onion": 16,
      "salt": {
        "length": 512
      }
    },
    "password": {
      "length": 8
    },
    "session": {
      "length": 512
    },
    "lockout": {
      "attempts": 5,
      "timeout": 300
    }
  }
}